reduce(function (a, b) {\n        return Math.round(a + b.cost);\n    }, 0);\n\n\n    if (item.length == 0) {\n        view = <div className={s.container_no_buy}>\n            <span>Ви ще не обрали замовлення</span>\n            <NavLink to={\"/profile/pizza\"} className={s.p}>Перейти в меню</NavLink>\n        </div>\n    } else {\n        view = <div className={s.container_buy}>\n            <div className={s.order}>\n                <span>Кошик</span>\n            </div>\n            {item.map(u => <div key={u._id}>\n\n                <div className={s.info}>\n\n                    <div className={s.container_photo}>\n                        {<img src={u.eat.photoURL} alt=\"\" height=\"75\" width=\"75\"/>}\n                    </div>\n\n\n                    <div className={s.your_order}>\n                        <div className={s.container_name}>\n                            {u.eat.name} {u.size > 0 ? \"-\" : \"\"} {u.size > 0 ? u.size : \"\"} {u.size > 0 ? \"см\" : \"\"}\n\n\n                        </div>\n\n                        <div className={s.amount}>\n                            <div className={s.border_minus}>\n                                <p className={s.minus} onClick={minus = () => {\n                                    if (u.eat.amount > 1) {\n                                        amountLeghtminus(1)\n                                    }\n                                    u.eat.amount = u.eat.amount > 1 ? u.eat.amount - 1 : u.eat.amount\n                                    u.cost = u.eat.cost * u.eat.amount\n\n                                }}>-</p>\n                            </div>\n                            <p className={s.number_amount}>{u.eat.amount}</p>\n                            <p className={s.plus} onClick={plus = () => {\n                                u.eat.amount = u.eat.amount + 1\n                                u.cost = (u.eat.cost) * u.eat.amount\n                                amountLeghtplus()\n                            }}>+</p>\n                        </div>\n\n                        <div className={s.costs}>\n                            <span className={s.x}> {u.eat.amount} x</span> <span\n                            className={s.amount_cost}>{Math.round(u.eat.cost)} грн</span>\n                        </div>\n\n                    </div>\n\n                    <span className={s.delete} onClick={delet = () => {\n                        let searchindex = u._id\n\n\n                        amountLeghtminus(itemState[itemState.findIndex(el => el._id === searchindex)].eat.amount)\n\n                        // @ts-ignore\n\n                        setItemState(itemState[itemState.findIndex(el => el._id === searchindex)].eat.amount = 1)\n\n                        setItemState(itemState.splice(itemState.findIndex((el => el._id === searchindex), 0)))\n\n\n                        deleteLenght(Number(lenght) > 0 ? Number(lenght) - 1 : null)\n\n\n                    }\n                    }>x</span>\n\n                </div>\n\n            </div>)}\n\n\n            <div className={s.all}>\n                <div className={s.all_cost_order}>\n                    <span className={s.text}>Разом:</span> <span className={s.all_cost}>{cost} грн</span>\n                </div>\n                <div className={s.button_order}>\n                    <NavLink to={\"/checkout\"} className={s.your_like}> Замовити</NavLink>\n                </div>\n            </div>\n\n\n        </div>\n    }\n\n\n    return (\n        <div className={s.content}>\n            {view}\n        </div>\n    )\n\n}\n\n\n","/Users/daloniil/Desktop/React/Pizza/pizzaProject/src/Selectors/buy-item-selector.ts",[],"/Users/daloniil/Desktop/React/Pizza/pizzaProject/src/Selectors/Menus-selector.ts",[],"/Users/daloniil/Desktop/React/Pizza/pizzaProject/src/Types/Type.ts",[],"/Users/daloniil/Desktop/React/Pizza/pizzaProject/src/Components/SliderItem/SliderItem.tsx",["295","296","297","298","299","300","301","302","303","304","